<div class="container">
<p id="notice"><%= notice %></p>

<p>
  <strong>Estate resource: </strong>
  <%= link_to "Here", @estate.url, :target => "_blank" %>
</p>

<p>
  <strong>Contact name: </strong>
  <%= @estate.name %>
</p>

<p>
  <strong>Contact number:</strong>
  <%= @estate.number %>
</p>

<p>
  <strong>Email:</strong>
  <%= @estate.email %>
</p>

<p>
  <strong>Address:</strong>
  <%= @estate.address %>
</p>

<p>
  <strong>Location :</strong>
  <%= @estate.latitude %>, <%= @estate.longitude %>
</p>


<p>
  <strong>Size:</strong>
  <%= @estate.size %>
</p>

<p>
  <strong>Price:</strong>
  <%= @estate.price %>
</p>

<%= link_to 'Edit', edit_estate_path(@estate) %> |
<%= link_to 'Back', estates_path %>


<script src="//maps.google.com/maps/api/js?key=AIzaSyB-njOYmM-TZiBmdwGZXuniH2rf5I1MWeg"></script>
<script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
<script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->

<br>
<div style="display:none;" >
  <%= link_to 'Show map', '#', :remote => true, :id => 'show_map'   %>
</div>


<div style='width: 800px;'>
  <div id="map" style='width: 800px; height: 400px;'></div>
</div>
</div>